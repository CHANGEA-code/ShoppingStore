tabs .order_main {
  color: #666;
}
tabs .order_main .order_item {
  padding: 20rpx;
  border-bottom: 1rpx solid #ccc;
}
tabs .order_main .order_item .order_no_row {
  padding: 10rpx 0;
  display: flex;
  justify-content: space-between;
}
tabs .order_main .order_item .order_price_row {
  padding: 10rpx 0;
  display: flex;
  justify-content: space-between;
}
tabs .order_main .order_item .order_price_row .order_price {
  color: var(--themeColor);
  font-size: 32rpx;
}
tabs .order_main .order_item .order_time_row {
  padding: 10 rpx 0;
  display: flex;
  justify-content: space-between;
}
